{
  "cli": {
    "version": ">= 9.0.7",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "ios-simulator": {
      "extends": "development",
      "ios": {
        "simulator": true
      },
      "channel": "ios-simulator"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "beta": {
      "distribution": "internal",
      "android": {
        "buildType": "app-bundle"
      },
      "channel": "beta"
    },
    "production": {
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
